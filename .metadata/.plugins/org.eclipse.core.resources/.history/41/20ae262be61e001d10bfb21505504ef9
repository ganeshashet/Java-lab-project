package Simple.Program;
import java.util.*;
import static java.lang.System.*;
public class SimilarElements {
	public static void main(String args[])
	{
		Scanner sc=new Scanner(System.in);
		out.println("enter the size of an array");
		int n=sc.nextInt();
		out.println("enter the array 1st  elements");
		String a[]=new String[n];
		for(int i=0;i<n;i++) 	
		{
			 a[i]=sc.next();
		}
		out.println("enter the array 2nd  elements");
		String b[]=new String[n];
		for(int i=0;i<n;i++) 	
		{
			 b[i]=sc.next();
		}
		for(int i=0;i<n;i++)
		{
			if(a[i]==b[i])
			{
				out.print(a[i]+"");
			}
		else
			{
				out.println("not have similar value");
			}
		}
		sc.close();
	}

}
